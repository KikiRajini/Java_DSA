class Solution {
    public boolean isAnagram(String s, String t) {
        if (s.length() != t.length()) return false;

        int[] store = new int[26];

        for (int i = 0; i < s.length(); i++) {
            store[s.charAt(i) - 'a']++;
            store[t.charAt(i) - 'a']--;
        }
        
        if (Arrays.equals(store,new int[26])) {return true;}

        return false;
    }
}

Character	ASCII Value
'A'	        65
'B'	        66
'a'	        97
'b'	        98
'0'	        48
'1'	        49
' ' (space)	32
